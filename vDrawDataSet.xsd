                         hba_draw AS d ON w.id = d.week_id INNER JOIN
                         hba_team AS t ON t.id = d.team_id INNER JOIN
                         hba_score_sheet AS s ON d.id = s.draw_id
WHERE        (w.season = YEAR(GETDATE()))
